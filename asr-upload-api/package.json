{
    "name": "nest-typescript-starter",
    "private": true,
    "version": "1.0.0",
    "description": "Nest TypeScript starter repository",
    "license": "MIT",
    "scripts": {
        "prebuild": "rimraf dist",
        "build": "nest build",
        "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
        "start": "nest start",
        "start:dev": "nest start --watch",
        "start:debug": "nest start --debug --watch",
        "start:prod": "node dist/src/main",
        "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
        "test": "jest",
        "test:watch": "jest --watch",
        "test:cov": "jest --coverage",
        "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
        "test:e2e": "jest --config ./test/jest-e2e.json",
        "docker:start": "cross-env NODE_ENV=production pm2-docker start process.yml --only api",
        "docker:start:dev": "sudo docker-compose -f docker-compose.yml -f docker-compose.dev.yml up",
        "docker:test": "docker-compose -f docker-compose.yml -f docker-compose.test.yml up --abort-on-container-exit",
        "migrate:create": "ts-node ./node_modules/typeorm/cli.js migration:create -n",
        "migrate:up": "ts-node ./node_modules/typeorm/cli.js migration:run",
        "migrate:down": "ts-node ./node_modules/typeorm/cli.js migration:revert"
    },
    "dependencies": {
        "@aginix/nestjs-gcloud-storage": "^1.2.0",
        "@golevelup/nestjs-rabbitmq": "^1.16.1",
        "@google-cloud/storage": "^5.8.5",
        "@nestjs/bull": "^0.3.1",
        "@nestjs/common": "^7.6.15",
        "@nestjs/config": "^0.5.0",
        "@nestjs/core": "^7.6.15",
        "@nestjs/mapped-types": "*",
        "@nestjs/microservices": "^8.4.7",
        "@nestjs/platform-express": "^7.6.17",
        "@nestjs/platform-fastify": "^7.6.15",
        "@nestjs/swagger": "^4.8.0",
        "@ntegral/nestjs-sentry": "2.0.9",
        "@sentry/node": "^6.15.0",
        "@sentry/tracing": "^6.15.0",
        "@svtslv/nestjs-ioredis": "^1.0.2",
        "@types/multer": "^1.4.5",
        "@types/uuid": "^8.3.0",
        "bigint-base-converter": "^0.1.2",
        "bull": "^3.22.5",
        "class-transformer": "0.4.0",
        "class-validator": "0.13.1",
        "dotenv": "^10.0.0",
        "fastify-swagger": "^4.7.0",
        "gif-frames": "^1.0.1",
        "ioredis": "^4.27.2",
        "kafka-streams": "^5.0.0",
        "kafkajs": "^1.15.0",
        "lodash": "^4.17.21",
        "multer-cloud-storage": "^2.7.1",
        "mysql": "^2.18.1",
        "nestjs-rate-limiter": "^2.7.4",
        "nestjs-typeorm-paginate": "^2.3.2",
        "npx": "^10.2.2",
        "parse-redis-url-simple": "^1.0.2",
        "reflect-metadata": "^0.1.13",
        "rimraf": "^3.0.2",
        "rxjs": "^6.6.6",
        "swagger-ui-express": "^4.1.6",
        "typeorm": "^0.2.32",
        "typeorm-adapter": "^1.3.0",
        "typeorm-model-generator": "^0.4.5",
        "uuid": "^8.3.2"
    },
    "devDependencies": {
        "@nestjs/cli": "^7.6.0",
        "@nestjs/schematics": "^7.3.1",
        "@nestjs/testing": "^7.6.15",
        "@nestjs/typeorm": "^7.1.5",
        "@types/bull": "^3.15.1",
        "@types/ioredis": "^4.26.3",
        "@types/jest": "^26.0.22",
        "@types/node": "^14.14.36",
        "@types/supertest": "^2.0.10",
        "@typescript-eslint/eslint-plugin": "^4.19.0",
        "@typescript-eslint/parser": "^4.19.0",
        "eslint": "^7.22.0",
        "eslint-config-prettier": "^8.1.0",
        "eslint-plugin-prettier": "^3.3.1",
        "jest": "^26.6.3",
        "prettier": "^2.2.1",
        "supertest": "^6.1.3",
        "ts-jest": "^26.5.4",
        "ts-loader": "^8.0.18",
        "ts-node": "^9.1.1",
        "tsconfig-paths": "^3.9.0",
        "typescript": "^4.2.3"
    },
    "jest": {
        "moduleFileExtensions": [
            "js",
            "json",
            "ts"
        ],
        "rootDir": "src",
        "testRegex": ".*\\.spec\\.ts$",
        "transform": {
            "^.+\\.(t|j)s$": "ts-jest"
        },
        "collectCoverageFrom": [
            "**/*.(t|j)s"
        ],
        "coverageDirectory": "../coverage",
        "testEnvironment": "node"
    }
}
