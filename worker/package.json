{
    "name": "nvk",
    "version": "1.0.0",
    "description": "",
    "main": "index.js",
    "scripts": {
        "build": "tsc && yarn build:resolve-module",
        "build:fix": "mv ./node_modules/sql-string-template/index.d.ts ./node_modules/sql-string-template/index.d.ts.rm",
        "build:restory_fix": "mv ./node_modules/sql-string-template/index.d.ts.rm ./node_modules/sql-string-template/index.d.ts",
        "build:resolve-module": "node bin/tsconfig",
        "prestart": "npm run build",
        "start": "node_modules/.bin/nodemon dist/src/index.js",
        "dev": "nodemon --watch src --ext .ts --exec 'node -r ts-node/register -r ./bin/tsconfig-paths-bootstrap.js --inspect --expose-gc src/index.ts'",
        "dev-event-dispatcher": "nodemon --watch src --ext .ts --exec 'node -r ts-node/register -r ./bin/tsconfig-paths-bootstrap.js --expose-gc src/index-event-dispatcher.ts'",
        "dev-worker": "nodemon --watch src --ext .ts --exec node -r ts-node/register -r ./bin/tsconfig-paths-bootstrap.js --expose-gc src/index-worker.ts",
        "test": "npx jest --runInBand --detectOpenHandles --colors --verbose --reporters=default",
        "lint": "eslint --ext ts src",
        "lint-fix": "eslint --fix --ext ts src",
        "test-client": "node node_modules/.bin/testapis e2e/client/index.yaml"
    },
    "keywords": [],
    "author": "",
    "license": "ISC",
    "importSort": {
        ".js, .jsx, .es6, .es": {
            "parser": "babylon",
            "style": "eslint",
            "options": {}
        },
        ".ts, .tsx": {
            "parser": "typescript",
            "style": "eslint",
            "options": {}
        }
    },
    "jest": {
        "moduleNameMapper": {
            "@/*": "<rootDir>/src/*",
            "#/*": "<rootDir>/core/*"
        }
    },
    "devDependencies": {
        "@babel/core": "^7.12.9",
        "@babel/plugin-proposal-decorators": "^7.12.1",
        "@babel/preset-env": "^7.12.7",
        "@babel/preset-typescript": "^7.12.7",
        "@types/bluebird": "^3.5.33",
        "@types/bull": "^3.14.4",
        "@types/ioredis": "^4.17.8",
        "@types/jest": "^26.0.15",
        "@types/lodash": "^4.14.165",
        "@types/node": "^14.14.10",
        "@types/pg": "^7.14.6",
        "@typescript-eslint/eslint-plugin": "^4.11.1",
        "@typescript-eslint/parser": "^4.11.1",
        "babel-jest": "^26.6.3",
        "babel-loader": "^8.0.2",
        "babel-plugin-transform-decorators": "^6.24.1",
        "eslint": "^7.17.0",
        "jest": "^26.6.3",
        "jest-junit": "^12.0.0",
        "nodemon": "^1.19.2",
        "testapis": "^1.0.4",
        "ts-jest": "^26.4.4",
        "ts-node": "^9.0.0",
        "typescript": "^4.1.2"
    },
    "dependencies": {
        "@lucafont2/onedrive-api": "^1.1.1",
        "@sentry/node": "^5.27.4",
        "amqp-ts": "^1.8.0",
        "async-mqtt": "^2.6.1",
        "axios": "^0.21.0",
        "bull": "^3.20.0",
        "change-case": "^4.1.2",
        "class-transformer": "0.4.0",
        "class-validator": "0.13.1",
        "dotenv": "^8.2.0",
        "fastest-validator": "^1.9.0",
        "geolib": "^3.3.1",
        "inversify": "^5.0.1",
        "inversify-inject-decorators": "^3.1.0",
        "ioredis": "^4.19.2",
        "kafka-streams": "^5.0.0",
        "lodash": "^4.17.20",
        "moment": "^2.29.1",
        "mqtt": "^4.2.8",
        "mysql": "^2.18.1",
        "onedrive-auth": "^1.1.1",
        "query-string": "^6.13.7",
        "reflect-metadata": "^0.1.13",
        "sentry": "^0.1.2",
        "sql-string-template": "https://github.com/khuyen2v/node-sql-string-template.git",
        "typeorm": "^0.2.29",
        "typeorm-model-generator": "^0.4.5",
        "uWebSockets.js": "github:uNetworking/uWebSockets.js#v18.9.0",
        "url": "^0.11.0",
        "winston": "^3.3.3"
    }
}
